          <BeanShellAssertion guiclass="BeanShellAssertionGui" testclass="BeanShellAssertion" testname="断言" enabled="true">
            <stringProp name="TestPlan.comments">验证当前上传插件包是否正确</stringProp>
            <stringProp name="BeanShellAssertion.query">import org.json.*; 

boolean flag = false;
String partName = vars.get(&quot;partName&quot;);
String vcname = vars.get(&quot;cname&quot;);
String vnote = vars.get(&quot;noteDe&quot;);
String response = prev.getResponseDataAsString();
JSONObject responseJson = new JSONObject(response); 
int totalStatus = responseJson.getInt(&quot;status&quot;);
JSONArray infoArrays = responseJson.getJSONArray(&quot;parts&quot;); 
for(int i=0; i&lt;infoArrays.length(); i++){  
	JSONObject responseInfo = (JSONObject)infoArrays.getJSONObject(i); 
     String name = responseInfo.getString(&quot;name&quot;);
	int status = responseInfo.getInt(&quot;status&quot;);
	String cname = responseInfo.getString(&quot;cname&quot;);
	String note = responseInfo.getString(&quot;note&quot;);
	log.info(&quot;vnote-----&quot;+vnote);
	log.info(&quot;解析后-----&quot;+note);
	if(partName.equals(name)&amp;&amp;status==0&amp;&amp;vcname.equals(cname)&amp;&amp;vnote.equals(note)){
		flag = true;
    		break; 
	} 
}
if(flag){
	Failure = false;  
    	FailureMessage = &quot;断言成功：请求数据响应正确&quot;; 
